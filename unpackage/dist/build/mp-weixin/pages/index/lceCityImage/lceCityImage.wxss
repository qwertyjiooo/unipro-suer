.content{background-color:#f0f8ff;height:100vh;overflow:auto}.content .tabTop{background-color:#fff;padding-bottom:20rpx;position:fixed;width:100%;top:0rpx;left:0;z-index:999}.content .tab{font-size:26rpx}.content .tab .tabText{font-size:26rpx;width:33.33%;text-align:center}.content .tab .tabText image{width:20rpx;height:20rpx}.content .tab .active{color:#1296db}.content .gridTag{background-color:#fff;position:absolute;z-index:1000;width:100%}.content .gridTag .gridContainer{text-align:center;padding:20rpx;display:grid;grid-template-columns:repeat(4,1fr);grid-gap:10px}.content .gridTag .gridContainer .gridItem{padding:20rpx;background-color:#f8f8f8;border-radius:100rpx;font-size:26rpx}.content .gridTag .gridContainer .Iactive{background-color:#1296db;color:#fff}.content .marginTop{margin-top:60rpx}.content .allCard{display:grid;grid-template-columns:repeat(2,1fr);padding-top:60rpx}.content .allCard .contentCard{width:calc(50vw - 40rpx);padding:20rpx 20rpx 0}.content .allCard .contentCard ._img{display:block;height:200rpx;width:100%;object-fit:cover;border-top-right-radius:10rpx;border-top-left-radius:10rpx}.content .allCard .contentCard .allImage{height:300rpx}.content .allCard .contentCard .textCard{background-color:#fff;line-height:64rpx;font-size:26rpx;padding:0 20rpx;display:-webkit-box;-webkit-line-clamp:1;-webkit-box-orient:vertical;overflow:hidden}