<view class="content"><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" circular="{{true}}" indicator-active-color="#ffffff"><block wx:for="{{swiperItemList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><swiper-item><image class="swiper-item _img" referrerPolicy="no-referrer" src="{{item.pageavater.url}}"></image></swiper-item></block></swiper><view class="home"><view class="iconList"><block wx:for="{{iconListimageList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator class="iconListimage" url="{{item.url}}"><image src="{{item.icon}}"></image><view class="text">{{''+item.text+''}}</view></navigator></block></view><view data-event-opts="{{[['tap',[['ParkingLot']]]]}}" class="activity" bindtap="__e"><view class="textA">不知道去哪玩？</view><view class="textB">想要了解定制旅游出行，点击这里可以以快速了解</view></view><view class="wizard"><view class="recommend">景点推荐</view><view class="tabulatingCard"><block wx:for="{{rankDateList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['fiche',['$0'],[[['rankDateList','',index,'_id']]]]]]]}}" class="fiche" style="{{'background-image:'+('url('+item.pageavater.url+')')+';'}}" bindtap="__e"><image class="ficheimage _img" src="{{recommendList[index].url}}"></image><view class="ficheText">{{''+item.title+''}}</view></view></block></view></view><view class="purchase"><view class="recommend">玩转尔滨<navigator class="span" url="/pages/index/MicroTravel/index">更多<image class="more" src="../../static/more.png" mode></image></navigator></view><scroll-view class="menu" scroll-x="true"><view data-event-opts="{{[['tap',[['allLoad',['$event']]]]]}}" class="{{['menu-item',(activeTab===0)?'active':'']}}" bindtap="__e">全部文娱</view><block wx:for="{{MicroTravelList}}" wx:for-item="item" wx:for-index="index" wx:key="_id"><view data-event-opts="{{[['tap',[['sift',['$0'],[[['MicroTravelList','_id',item._id,'_id']]]]]]]}}" class="{{['menu-item',(activeTab===item._id)?'active':'']}}" bindtap="__e">{{item.title}}</view></block></scroll-view><view class="waFall"><block wx:for="{{NavMicroTravelKist}}" wx:for-item="item" wx:for-index="Index" wx:key="_id"><view data-event-opts="{{[['tap',[['moreDetails',['$0'],[[['NavMicroTravelKist','_id',item._id,'_id']]]]]]]}}" class="item" bindtap="__e"><image src="{{item.images[0].url}}" mode="widthFix"></image><view class="item_text">{{item.title}}</view></view></block></view><block wx:if="{{$root.g0===0}}"><view style="text-align:center;padding:50rpx 0;font-size:32rpx;">暂无数据</view></block></view></view></view>