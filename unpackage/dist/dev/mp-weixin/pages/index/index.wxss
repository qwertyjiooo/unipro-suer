.content {
  font-size: 26rpx;
  height: 100vh;
  background-color: #f6fbff;
  overflow: auto;
  position: relative;
}
.content .startStop {
  position: absolute;
  top: 0;
  left: 0;
}
.content .startStop button {
  width: 100%;
  height: 200rpx;
  background-color: #999;
  margin-top: 20rpx;
}
.content swiper {
  width: 100vw;
  height: 400rpx;
  margin-bottom: 20rpx;
  z-index: -1;
}
.content .swiper-item {
  background-color: pink;
  width: 100%;
  top: 0;
  left: 0;
}
.content .home {
  padding: 0 20rpx;
}
.content .home .iconList {
  height: 150rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.content .home .iconList .iconListimage {
  flex: 1;
  text-align: center;
}
.content .home .iconList .iconListimage image {
  width: 80rpx;
  height: 80rpx;
}
.content .home .iconList .iconListimage .text {
  font-size: 26rpx;
  line-height: 48rpx;
  color: #999;
}
.content .home .activity {
  height: 100rpx;
  background-color: pink;
  border-radius: 20rpx;
  color: #ffffff;
  padding: 20rpx;
  box-shadow: #999;
  margin-bottom: 40rpx;
  margin-top: 20rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% auto;
  box-shadow: 0px 0px 10px 0px rgba(0, 0, 0, 0.5);
}
.content .home .activity .textA {
  font-size: 32rpx;
}
.content .home .activity .textB {
  font-size: 26rpx;
  line-height: 30rpx;
}
.content .home .recommend {
  font-weight: bold;
  font-size: 32rpx;
  line-height: 64rpx;
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.content .home .wizard {
  margin-bottom: 20rpx;
}
.content .home .wizard .tabulatingCard {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.content .home .wizard .tabulatingCard .fiche {
  width: 25%;
  height: 200rpx;
  padding: 20rpx;
  position: relative;
  display: flex;
  align-items: flex-end;
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
}
.content .home .wizard .tabulatingCard .fiche .backimage {
  position: absolute;
  object-fit: cover;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.content .home .wizard .tabulatingCard .fiche .ficheimage {
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  top: -10rpx;
}
.content .home .wizard .tabulatingCard .fiche .ficheText {
  z-index: 1000;
  width: 100%;
  display: inline-block;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #ffffff;
  font-size: 26rpx;
}
.content .home .purchase .span {
  height: 100%;
  padding-left: 20rpx;
  display: flex;
  font-size: 26rpx;
  align-items: center;
  justify-content: center;
  font-weight: 400;
  color: #999;
}
.content .home .purchase .more {
  width: 26rpx;
  height: 26rpx;
  color: #999;
}
.content .home .purchase .waFall {
  margin-bottom: 20rpx;
  -webkit-column-count: 2;
          column-count: 2;
  -webkit-column-gap: 20rpx;
          column-gap: 20rpx;
}
.content .home .purchase .waFall .item {
  /* 或者根据需要设置合适的宽度百分比 */
  margin-bottom: 20rpx;
  -webkit-column-break-inside: avoid;
          break-inside: avoid;
  /* 每个项目之间的间距 */
}
.content .home .purchase .waFall .item .item_text {
  text-align: center;
  padding: 10rpx;
  font-size: 26rpx;
  background-color: #ffffff;
  box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(0, 0, 0, 0.2);
}
.content .home .purchase .waFall .item image {
  width: 100%;
  display: block;
}
.content .home .purchase .menu {
  white-space: nowrap;
  /* 防止换行 */
}
.content .home .purchase .menu .menu-item {
  margin-bottom: 20rpx;
  display: inline-block;
  /* 将菜单项显示为行内块元素 */
  margin-right: 20rpx;
  /* 根据需求进行调整 */
  border-radius: 40rpx;
  /* 根据需求进行调整 */
  cursor: pointer;
  /* 当作按钮处理时，可以改变光标形状 */
  font-size: 26rpx;
  padding: 15rpx;
  background-color: #e6e6e6;
  font-weight: 300;
}
.content .home .purchase .menu .active {
  background-color: #1296db;
  color: #ffffff;
}

